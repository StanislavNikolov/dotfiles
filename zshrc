# simple movement ones
alias c='cd'
alias l='ls'
alias lh='ls -lh'
alias la='ls -a'
alias lah='ls -lah'

# non-standart commands
alias vi='nvim'

# other small gems
alias duh='du -h --max-depth=1'

# source ~/.local/bin/bashmarks.sh

export EDITOR="nvim"
export PATH="$HOME/.hax:$HOME/.local/bin:$PATH"
